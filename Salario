import java.util.Scanner;

public class Main {
    static int salarioMinimo;
    static int salariofuncionario;
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Digite o salario minimo" );
        salarioMinimo = scanner.nextInt();

        System.out.println("Digite o salario do funcionario" );
        salariofuncionario = scanner.nextInt();

        int quantosSalarios = (salariofuncionario / salarioMinimo);


        System.out.println(quantosSalarios);

    }
}
