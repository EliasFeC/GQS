import java.util.Scanner;

public class Main {
    static int primeiroCateto;
    static int segundoCateto;
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Digite o primeiro cateto" );
        primeiroCateto = scanner.nextInt();

        System.out.println("Digite o segundo cateto" );
        segundoCateto = scanner.nextInt();

        int hipotenusa = (int) (Math.pow(primeiroCateto, 2) + Math.pow(segundoCateto , 2));


        System.out.println(hipotenusa);

    }
}
